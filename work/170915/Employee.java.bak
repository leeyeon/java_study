public class Employee  
{	
	String part;
	String name;
	int age;
	int baseSalary;

	public Employee()
	{
		System.out.println("Emp의 default Construtor");
	}

	public Employee(String name)
	{
		this.name = name;
		System.out.println("Emp의 name을 받는 Construtor");
	}

	public Employee(String name, String part)
	{	
		this(name);
		this.part = part;
		System.out.println("Emp의 name,part을 받는 Construtor");
	}

	public Employee(String name, String part,int age)
	{	
		this(name,part);
		this.age = age;
		System.out.println("Emp의 name,part,age을 받는 Construtor");
	}
	public int salary() 
	{
		System.out.println("Emp의 satary() method");
		baseSalary = 100;
		return baseSalary;
	}
}
