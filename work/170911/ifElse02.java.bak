public class IfElse02
{
	public static void main(String[] args)
	{
		int x = 1;
		int y = 2;

		if(x==3 & y ==2) {
			System.out.println("x = 1 이고 y = 2 입니다.");
		}

		if(x==1 | y==3) {
			System.out.println("x = 1 이거나 y = 3 입니다.");
		}

		if(x==3 && y==2) {
			System.out.println("x = 3 이고 y = 2 입니다.");
		}

		if(x==1 || y==3) {
			System.out.println("x = 1 이거나 y = 3 입니다.");
		}
	}
}